page {
  background: #f5f5f4;
  font-size: 24rpx;
  height: 100%;
  overflow-y: hidden;
}
.mar-top {
  margin-top: 24rpx;
}
.mar-bottom {
  margin-bottom: 24rpx;
}
.shape-normal {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
}
.pad-box {
  padding: 30rpx;
  background: #fff;
}
.bottom-line {
  border-bottom: 1rpx solid #f5f5f4;
}
.top-line {
  border-top: 1rpx solid #f5f5f4;
}

.course-photo {
  width:210rpx;
  height:148rpx;
  margin-right: 26rpx;
  border-radius:14rpx;
}
.course-photo > image {
  width: 100%;
  height: 100%;
  background: #f2aa46;
  border-radius:14rpx;
}
