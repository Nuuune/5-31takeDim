.header {
  width: 100%;
  height: 284rpx;
  position: relative;
}
.header text {
  color:#79ccca;
  font-size:30rpx;
}
.header image {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: -1;
}
.header .info {
  width: 100%;
}
.header .info > view {
  width: 100%;
}
.header .info .avater image{
  position: static;
  width: 100%;
  height: 100%;
  width: 120rpx;
  height: 120rpx;
  border: 4rpx solid #79ccca;
  border-radius: 50%;
}
.header .info text{
  width: 100%;
  color: #e1f6f9;
  text-align: center;
}
.login-btn {
  padding:10rpx 20rpx;
  border:2rpx solid #79ccca;
  border-radius:14rpx;
}
page {
  background: #f5f5f4;
  font-size:32rpx;
  height: 100%;
  overflow-y: hidden;
}
.mar-top {
  margin-top: 24rpx;
}
.mar-bottom {
  margin-bottom: 24rpx;
}
.shape-normal {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
}
.pad-box {
  padding: 30rpx;
  background: #fff;
}
.bottom-line {
  border-bottom: 3rpx solid #f5f5f4;
}
.top-line {
  border-top: 3rpx solid #f5f5f4;
}
.lf-pad-box {
  padding:0 30rpx;
}
.boder-c-gray {
  border-color: #eaeaea;
}
.right-mar {
  margin-right: 20rpx;
}
